{
  "name": "blaze-pulse",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn reset:dir && node ./esbuild/build.mjs",
    "clean:tsc": "rm -rf dist && tsc",
    "watch": "yarn reset:dir && concurrently \"node ./esbuild/watch.mjs\"",
    "test": "jest",
    "reset:dir": "rm -rf dist && mkdir dist",
    "lint:lambda": "eslint src/**/*.ts --fix --cache",
    "lint:infra": "eslint infra/**/*.ts --fix --cache",
    "lint": "yarn lint:lambda && yarn lint:infra",
    "format:src": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:infra": "prettier --config .prettierrc 'infra/**/*.ts' --write",
    "format": "yarn format:src && yarn format:infra",
    "build:wac": "npx ghawac build && git add .github/workflows/*.yml",
    "cdk": "cdk",
    "postinstall": "husky"
  },
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "devDependencies": {
    "@getgreenline/eslint-config-greenline": "^1.1.1",
    "@getgreenline/github-actions-wac": "^1.0.0",
    "@getgreenline/infra-utils": "^2.0.0",
    "@parcel/watcher": "^2.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.4",
    "@types/node": "20.5.9",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "babel-jest": "^29.6.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.18.10",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.6.4",
    "lint-staged": "^15.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.11.0",
    "@aws-lambda-powertools/metrics": "^1.11.0",
    "@aws-lambda-powertools/parameters": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^1.11.0",
    "@aws-sdk/client-athena": "^3.540.0",
    "@aws-sdk/client-dynamodb": "^3.484.0",
    "@aws-sdk/client-eventbridge": "^3.533.0",
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/client-secrets-manager": "^3.525.0",
    "@aws-sdk/client-sqs": "^3.536.0",
    "@aws-sdk/client-sts": "^3.535.0",
    "@aws-sdk/lib-storage": "^3.537.0",
    "@aws-sdk/util-dynamodb": "^3.556.0",
    "@middy/core": "^5.3.2",
    "@middy/sqs-partial-batch-failure": "^5.3.2",
    "@slack/webhook": "^7.0.2",
    "csv-stringify": "^6.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "dynamoose": "^4.0.0",
    "fast-csv": "^4.3.6",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.5.3",
    "reflect-metadata": "^0.2.2",
    "serialize-error": "^11.0.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  }
}
