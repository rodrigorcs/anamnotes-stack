{
  "name": "anamnotes-stack",
  "version": "0.1.0",
  "scripts": {
    "build": "yarn reset:dir && node ./esbuild/build.mjs",
    "clean:tsc": "rm -rf dist && tsc",
    "watch": "yarn reset:dir && concurrently \"node ./esbuild/watch.mjs\"",
    "test": "jest",
    "reset:dir": "rm -rf dist && mkdir dist",
    "lint:lambda": "eslint src/**/*.ts --fix --cache",
    "lint:infra": "eslint infra/**/*.ts --fix --cache",
    "lint": "yarn lint:lambda && yarn lint:infra",
    "format:src": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:infra": "prettier --config .prettierrc 'infra/**/*.ts' --write",
    "format": "yarn format:src && yarn format:infra",
    "cdk": "cdk",
    "postinstall": "husky"
  },
  "lint-staged": {
    "**/*.{js,ts,json}": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "devDependencies": {
    "@parcel/watcher": "^2.2.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.4",
    "@types/node": "20.5.9",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-jest": "^29.6.2",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.18.10",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-tsc": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.6.4",
    "lint-staged": "^15.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.11.0",
    "@aws-lambda-powertools/metrics": "^1.11.0",
    "@aws-lambda-powertools/parameters": "^2.0.0",
    "@aws-lambda-powertools/tracer": "^1.11.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.600.0",
    "@aws-sdk/client-eventbridge": "^3.600.0",
    "@aws-sdk/client-secrets-manager": "^3.600.0",
    "@middy/core": "^5.3.2",
    "@middy/http-cors": "^5.4.1",
    "@middy/http-error-handler": "^5.4.1",
    "@middy/sqs-partial-batch-failure": "^5.3.2",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "dynamoose": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21"
  }
}
